{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"module": "commonjs",
		"noEmit": true,
		"pretty": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es5"
	},
	"include": ["./src", "gatsby"],
	"exclude": ["./node_modules", "./public", "./.cache"]
}
